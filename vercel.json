{
    "version": 2,
    "buildCommand": "next build",
    "devCommand": "next dev",
    "installCommand": "npm install",
    "outputDirectory": ".next",
    "rewrites": [
      {
        "source": "/api-proxy/:path*",
        "destination": "http://13.61.61.180:8080/api/v1/:path*"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "http://13.61.61.180:8080/api/v1",
      "NEXT_PUBLIC_IMAGE_URL": "http://13.61.61.180:8080/api/v1/images/view"
    },
    "build": {
      "env": {
        "NEXT_PUBLIC_API_URL": "http://13.61.61.180:8080/api/v1",
        "NEXT_PUBLIC_IMAGE_URL": "http://13.61.61.180:8080/api/v1/images/view"
      }
    }
  }